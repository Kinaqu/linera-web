{
    "name" : "linera-web",
    "version" : "1.0",
    "description" : "linera-web",
    "permissions": [],
    "content_scripts": [
        {
            "matches": ["file:///*"],
            "js": ["content_script.js"]
        }
    ],
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval';"
    },
    "manifest_version": 3
}
